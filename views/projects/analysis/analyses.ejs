<%- include("../../partials/header") %>

<h1 class="title"><%= `Analyses for Project ${projectid}` %></h1>

<table
  id="tab"
  class="ui striped selectable celled compact table"
  style="width:100%"
>
  <thead>
    <tr>
      <th>N</th>
      <th>ID</th>
      <th>TimeCreated</th>
      <th>TimeModified</th>
      <th>OwnerID</th>
      <th>TenantID</th>
      <th>TenantName</th>
      <th>Reference</th>
      <th>UserReference</th>
      <th>PipelineID</th>
      <th>Status</th>
      <th>StartDate</th>
      <th>EndDate</th>
      <th>Summary</th>
    </tr>
  </thead>
  <tbody>
    <% p = data["items"]; %>
    <% firstPageToken = data["firstPageToken"] %>
    <% nextPageToken = data["nextPageToken"] %>
    <% prevPageToken = data["prevPageToken"] %>
    <% if (firstPageToken) {%>
        <a href="/projects/<%= projectid %>/analyses?pageToken=<%= firstPageToken %>" class="ui toggle compact blue button">FirstPage</a>
    <% } %>
    <% if (nextPageToken) {%>
        <a href="/projects/<%= projectid %>/analyses?pageToken=<%= nextPageToken %>" class="ui toggle compact green button">NextPage</a>
    <% } %>
    <% if (prevPageToken) {%>
        <a href="/projects/<%= projectid %>/analyses?pageToken=<%= prevPageToken %>" class="ui toggle compact orange button">PreviousPage</a>
    <% } %>
    <% for (let i = 0; i < p.length; i++) { %>
    <tr>
      <td><%= `${i + 1}.` %></td>
      <td><a href="/projects/<%= projectid %>/analyses/<%= p[i].id %>"><%= p[i].id %></a></td>
      <td><%= p[i].timeCreated %></td>
      <td><%= p[i].timeModified %></td>
      <td><%= p[i].ownerId %></td>
      <td><%= p[i].tenantId %></td>
      <td><%= p[i].tenantName %></td>
      <td><%= p[i].reference %></td>
      <td><%= p[i].userReference %></td>
      <td><%= p[i].pipeline.id %></td>
      <td><%= p[i].status %></td>
      <td><%= p[i].startDate %></td>
      <td><%= p[i].endDate %></td>
      <td><%= p[i].summary %></td>
    </tr>
    <% } %>
  </tbody>
</table>

<script>
  $(document).ready(function() {
    $("#tab").DataTable({
      pageLength: 10,
      lengthMenu: [ [10, -1], [10, "All"] ],
      "dom": '<"top"flip>t<"bottom"flip>'
    });
  });
</script>

<pre>
<%- jsonSyntaxHighlight(JSON.stringify(data, null, 2)) %>
</pre>

<%- include("../../partials/footer") %>
