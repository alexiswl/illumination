<%- include("partials/header") %>

<h1 class="title"><%= `Regions` %></h1>

<table
  id="tab"
  class="ui striped selectable celled compact table"
  style="width:100%"
>
  <thead>
    <tr>
      <th>N</th>
      <th>Region</th>
      <th>TenantName</th>
      <th>CountryTenantName</th>
      <th>TimeCreated</th>
      <th>TimeModified</th>
    </tr>
  </thead>
  <tbody>
    <% p = data["items"]; %>
    <% for (let i = 0; i < p.length; i++) { %>
    <tr>
      <td><%= `${i + 1}.` %></td>
      <td><%= `${p[i].cityName}, ${p[i].country.name}  (${p[i].code}), ${p[i].country.region}` %></td>
      <td><%= p[i].tenantName %></td>
      <td><%= p[i].country.tenantName %></td>
      <td><%= p[i].timeCreated %></td>
      <td><%= p[i].timeModified %></td>
    </tr>
    <% } %>
  </tbody>
</table>

<script>
  $(document).ready(function() {
    $("#tab").DataTable({
      pageLength: 10,
      lengthMenu: [ [10, -1], [10, "All"] ],
      "dom": '<"top"flip>t<"bottom"flip>'
    });
  });
</script>

<pre>
<%- jsonSyntaxHighlight(JSON.stringify(data, null, 2)) %>
</pre>

<%- include("partials/footer") %>

