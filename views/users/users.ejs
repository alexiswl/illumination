<%- include("../partials/header") %>

<h1 class="title"><%= `Users` %></h1>

<table
  id="tab"
  class="ui striped selectable celled compact table"
  style="width:100%"
>
  <thead>
    <tr>
      <th>N</th>
      <th>Email</th>
      <th>ID</th>
      <th>ID2</th>
      <th>OwnerID</th>
      <th>TenantID</th>
      <th>UserName</th>
      <th>TimeCreated</th>
      <th>TimeModified</th>
    </tr>
  </thead>
  <tbody>
    <% p = data["items"]; %>
    <% for (let i = 0; i < p.length; i++) { %>
    <tr>
      <td><%= `${i + 1}.` %></td>
      <td><%= p[i].email %></td>
      <td><a href="/users/<%= p[i].id %>"><%= p[i].id %></a></td>
      <td><%= id2username(p[i].ownerId) %></td>
      <td><%= p[i].ownerId %></td>
      <td><%= p[i].tenantId %></td>
      <td><%= p[i].username %></td>
      <td><%= p[i].timeCreated %></td>
      <td><%= p[i].timeModified %></td>
    </tr>
    <% } %>
  </tbody>
</table>

<script>
  $(document).ready(function() {
    $("#tab").DataTable({
      pageLength: 20,
      lengthMenu: [ [10, 20, 50, -1], [10, 20, 50, "All"] ],
      "dom": '<"top"flip>t<"bottom"flip>'
    });
  });
</script>

<pre>
<%- jsonSyntaxHighlight(JSON.stringify(data, null, 2)) %>
</pre>

<%- include("../partials/footer") %>
